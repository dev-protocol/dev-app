{
	"version": 2,
	"alias": "devtoken.rocks",
	"builds": [
		{ "src": "build/src/index.js", "use": "@now/node" },
		{ "src": "public", "use": "@now/static" }
	],
	"routes": [
		{
			"src": "/alpha/.*",
			"status": 301,
			"headers": {
				"Location": "/"
			}
		},
		{
			"src": "/public/(.*)",
			"dest": "/public/$1"
		},
		{
			"src": ".*",
			"dest": "build/src/index.js"
		}
	]
}
